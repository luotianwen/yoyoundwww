	<cache name="fkqgs" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="rxphs" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="scrxs" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="scjps" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="xps" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="getfoolrcategorys" maxEntriesLocalHeap="1000" eternal="true" overflowToDisk="true" statistics="true"/>
