	<cache name="goods" maxEntriesLocalHeap="10000" eternal="true" overflowToDisk="true" statistics="true"/>
	<cache name="imgs" maxEntriesLocalHeap="10000" eternal="true" overflowToDisk="true" statistics="true"/>
